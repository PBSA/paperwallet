{
  "name": "ppytest-paperwallet",
  "version": "0.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel app.js --presets babel-preset-es2015 --out-dir lib",
    "build:watch": "babel --watch app.js --presets babel-preset-es2015 --out-dir lib",
    "prebrowserify": "npm run build",
    "postbrowserify": "uglifyjs --compress --mangle --sequences --drop_console --output dist/bundle.min.js -- dist/bundle.js",
    "browserify": "browserify lib/app.js -o dist/bundle.js -d",
    "archive": "cp -rf dist releases/$npm_package_name-$npm_package_version && cd releases && zip -r $npm_package_name-$npm_package_version.zip $npm_package_name-$npm_package_version",
    "checksums": "sha256sum releases/$npm_package_name-$npm_package_version.zip && sha1sum releases/$npm_package_name-$npm_package_version.zip",
    "tag": "git tag $npm_package_name-$npm_package_version && git push --tags",
    "gh-pages": "git checkout gh-pages && cp -vfR releases/$npm_package_name-$npm_package_version/* . && git commit -am \"[deploy] $npm_package_version\" && git push origin gh-pages && git checkout master",
    "release": "npm run tag && npm run archive && npm run checksums"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bitsharesjs": "latest",
    "babelify": "^7.3.0",
    "jquery": "^3.0.0",
    "qrcode": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.1.0",
    "uglify-js": "^2.7.0"
  }
}
